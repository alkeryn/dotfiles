#!/usr/bin/env bash
export mainpc=false
export laptop=false
export mainpcenv
export PC
[ -d /sys/class/power_supply/BAT0 ] && { laptop=true;PC=laptop; } ||  { mainpc=true;PC=mainpc; }
#grep -q Alkeryn-PC /etc/hostname && { mainpc=true;PC=mainpc; }
#grep -q Blade /etc/hostname && { laptop=true;PC=laptop; }

[[ -z "$PC" ]] && { mainpc=true;PC=mainpc; }

true
